{
  "fmt": {
    "options": {
      "indentWidth": 2
    }
  },
  "tasks": {
    "test": "deno test --parallel --coverage=cov_profile",
    "coverage": "deno coverage cov_profile",
    "test:coverage": "deno task test && deno task coverage",
    "test:cov": "deno task test:coverage"
  },
  "imports": {
    "~/": "./src/",
    "#/": "./"
  }
}
